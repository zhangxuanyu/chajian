 <canvas disable-scroll="true" canvas-id="myCanvasPic2" style="width:100%;height:896rpx;margin:0 auto;box-sizing:border-box;z-index:0;position:fixed;top:0;left:0" ></canvas>
 <canvas disable-scroll="true" canvas-id="myCanvasPic1" style="width:100%;height:896rpx;margin:0 auto;box-sizing:border-box;z-index:1;" ></canvas>
 <canvas disable-scroll="true" canvas-id="myCanvas1" style="width:100%;height:896rpx;margin:0 auto;box-sizing:border-box;z-index:2;position:fixed;top:0;left:0" bindtouchstart='start' bindtouchmove='move' bindtouchend='end'/>  
 
<!--提示  -->
<image src='../../image/modify_tips.png' style='width:470rpx;height:32rpx;position:fixed;top:1040rpx;left:140rpx;'></image>
<!--操作  -->
<view>
  <view class='back' bindtap='back'>
    <image src='../../image/back.png' style='width:98rpx;height:98rpx'></image>
  </view>
  <view class='go' bindtap='go'>
    <image src='../../image/go.png' style='width:98rpx;height:98rpx'></image>
  </view>
  <view class='review' bindtap='review'>
    <image src='../../image/review.png' style='width:98rpx;height:98rpx'></image>
  </view>
  <view class='remodification'>
    <image src='{{fgSrc}}' style='height:44rpx;width:30rpx;margin-left:-2rpx;margin-top:32rpx;' ></image>
    <image src='{{bgSrc}}' style='height:44rpx;width:30rpx;margin-left:114rpx'></image>
    <view bindtap='increaseFg' style='position:fixed;width:110rpx;height:110rpx;top:1096rpx;left:60rpx'></view>
    <view bindtap='increaseBg' style='position:fixed;width:110rpx;height:110rpx;top:1096rpx;left:204rpx'></view>
  </view>
  <view class='productPicture' bindtap='productPicture'>
    <image src='../../image/productPicture.png' style='height:110rpx;width:375rpx;'></image>
  </view>
</view>
